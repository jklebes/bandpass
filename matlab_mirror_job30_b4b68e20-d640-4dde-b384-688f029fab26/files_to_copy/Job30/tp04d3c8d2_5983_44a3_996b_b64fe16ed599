#!/bin/sh
export PARALLEL_SERVER_DECODE_FUNCTION='parallel.cluster.generic.communicatingDecodeFcn'
export PARALLEL_SERVER_STORAGE_CONSTRUCTOR='makeFileStorageObject'
export PARALLEL_SERVER_JOB_LOCATION='Job30'
export PARALLEL_SERVER_MATLAB_EXE='/cluster/sw/matlab/R2021b/bin/matlab/bin/worker'
export PARALLEL_SERVER_MATLAB_ARGS='-parallel'
export PARALLEL_SERVER_DEBUG='true'
export MLM_WEB_LICENSE='false'
export PARALLEL_SERVER_STORAGE_LOCATION='/cluster/cjweijer_group/jklebes001//jklebes001/R2022a'
export PARALLEL_SERVER_CMR='/cluster/sw/matlab/R2021b/bin/matlab'
